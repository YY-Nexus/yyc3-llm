{
  "name": "yanyu-cloud-deepstack",
  "version": "1.0.0",
  "description": "言語云³深度堆栈全栈智创引擎",
  "private": true,
  "type": "module",
  "scripts": {
    "test:e2e": "playwright test e2e/model-engine.spec.ts",
    "test:e2e:ui": "playwright test e2e/model-engine.spec.ts --ui",
    "dev": "next dev --port 3690",
    "build": "next build",
    "start": "next start --port 3690",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "type-check:web": "tsc --noEmit -p tsconfig.web-only.json",
    "setup:env": "tsx scripts/setup-core-environment.ts",
    "validate:env": "tsx scripts/validate-environment.ts",
    "check:ollama": "tsx scripts/check-ollama-service.ts",
    "install:models": "tsx scripts/install-ollama-models.ts",
    "dev:full": "npm run setup:env && npm run dev",
    "start:production": "npm run build && npm start",
    "clean": "rm -rf .next out dist",
    "reset:env": "rm -f .env.local && npm run setup:env",
    "test": "jest --config jest.config.cjs --passWithNoTests",
    "test:watch": "jest --config jest.config.cjs --watch --passWithNoTests",
    "test:coverage": "jest --config jest.config.cjs --coverage --passWithNoTests",
    "test:ci": "jest --config jest.config.cjs --ci --coverage --watchAll=false --passWithNoTests",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "validate": "npm run type-check && npm run lint && npm run test",
    "package:web": "./scripts/package-app.sh web",
    "package:docker": "./scripts/package-app.sh docker",
    "package:electron": "./scripts/package-app.sh electron",
    "package:pwa": "./scripts/package-app.sh pwa",
    "package:all": "./scripts/package-app.sh all",
    "package:clean": "./scripts/package-app.sh clean",
    "size:check": "size-limit"
  },
  "dependencies": {
